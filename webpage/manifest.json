{
    "manifest_version": 3,
    "name": "Confidently",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://mail.google.com/*"
        ],
        "js": ["script.js"]
      }
    ],
    "permissions":[
      "activeTab", 
      "http://127.0.0.1:5000"
    ]
    
  } 